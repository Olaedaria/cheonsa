<styles>

	<frame
		key="e_window_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 27"
		texture_map_fill_middle="true"/>

	<text
		key="e_window_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="16.0"
		align_horizontal="left"
		align_vertical="center"/>

	<frame
		key="e_panel_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 1"
		texture_map_fill_middle="true"/>

	<text
		key="e_label_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="14.0"
		align_horizontal="left"
		align_vertical="center"/>

	<frame
		key="e_field_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 105"
		texture_map_fill_middle="true"/>

	<text
		key="e_field_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="16.0"
		align_horizontal="left"
		align_vertical="center"/>

	<frame
		key="e_button_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 53"
		texture_map_fill_middle="true"/>

	<text
		key="e_button_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="16.0"
		align_horizontal="center"
		align_vertical="center"/>

	<text
		key="e_button_text_small"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="12.0"
		align_horizontal="center"
		align_vertical="center"/>

	<frame
		key="e_button_priority_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 79"
		texture_map_fill_middle="true"/>

	<text
		key="e_button_priority_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="16.0"
		weight="0.0125"
		align_horizontal="center"
		align_vertical="center"/>

	<frame
		key="e_check_mark_frame"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="8 8 8 8"
		texture_map_origin="1 105"
		texture_map_fill_middle="true"/>

	<frame
		key="e_scroll_rail_frame"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 1"
		texture_map_fill_middle="true"/>

	<frame
		key="e_scroll_grip_frame"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="1 1"
		texture_map_fill_middle="true"/>

	<frame
		key="e_list_item_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 79"
		texture_map_fill_middle="true"/>

	<frame
		key="e_list_item_selected_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 27"
		texture_map_fill_middle="true"/>

	<frame
		key="e_list_item_mouse_selected_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 27"
		texture_map_fill_middle="true"/>

	<frame
		key="e_list_item_outlined_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 1"
		texture_map_fill_middle="true"/>

	<text
		key="e_list_item_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="14.0"
		offset="0.0 0.0"
		skew="0.0"
		weight="0.0"
		softness="0.0"
		paragraph_spacing="5.0"
		line_spacing="0.0"
		glyph_spacing="0.0"
		align_horizontal="left"
		align_vertical="center"/>

	<frame
		key="e_combo_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="50 24"
		texture_map_edges="4 4 25 4"
		texture_map_origin="1 157"
		texture_map_fill_middle="true"/>

	<frame
		key="e_collection_column_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 53"
		texture_map_fill_middle="true"/>

	<text
		key="e_collection_column_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="10.0"
		offset="0.0 0.0"
		skew="0.0"
		weight="0.0"
		softness="0.0"
		paragraph_spacing="5.0"
		line_spacing="0.0"
		glyph_spacing="0.0"
		align_horizontal="left"
		align_vertical="center"/>

	<frame
		key="e_collection_item_selected_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="nine_slice_stretch"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 27"
		texture_map_fill_middle="true"/>

	<frame
		key="e_collection_item_icon_frame"
		pixel_shader="menu_ps_frame_keyed.hlsl"
		texture="menus/atlas.png"
		texture_map_mode="scale_to_fit"
		texture_map_size="24 24"
		texture_map_edges="4 4 4 4"
		texture_map_origin="79 28"
		texture_map_fill_middle="true"/>

	<text
		key="e_collection_item_text"
		font="menus/DXLBaB-KSCpc-EUC-H.ttf"
		size="12.0"
		offset="0.0 0.0"
		skew="0.0"
		weight="0.0"
		softness="0.0"
		paragraph_spacing="5.0"
		line_spacing="0.0"
		glyph_spacing="0.0"/>

	<map key="e_window">
		<entry element_name="frame" style_key="e_window_frame"/>
		<entry element_name="text" style_key="e_window_text"/>
		<entry element_name="client_panel" style_key="[invisible]"/>
	</map>

	<map key="e_panel">
		<entry element_name="frame" style_key="e_panel_frame"/>
	</map>

	<map key="e_label">
		<entry element_name="frame" style_key="[invisible]"/>
		<entry element_name="text" style_key="e_label_text"/>
	</map>

	<map key="e_text">
		<entry element_name="frame" style_key="e_field_frame"/>
		<entry element_name="text" style_key="e_field_text" anchor_measures="4 4 4 4"/>
	</map>

	<map key="e_button">
		<entry element_name="frame" style_key="e_button_frame"/>
		<entry element_name="text" style_key="e_button_text_p"/>
	</map>

	<map key="e_button_frameless">
		<entry element_name="frame" style_key="[invisible]"/>
		<entry element_name="text" style_key="e_button_text"/>
	</map>

	<map key="e_button_frameless_small">
		<entry element_name="frame" style_key="[invisible]"/>
		<entry element_name="text" style_key="e_button_text_small"/>
	</map>

	<map key="e_button_priority">
		<entry element_name="frame" style_key="e_button_priority_frame"/>
		<entry element_name="text" style_key="e_button_priority_text"/>
	</map>

	<map key="e_check">
		<entry element_name="box_frame" style_key="e_field_frame" anchor="left top bottom" anchor_measures="0 0 24 0"/>
		<entry element_name="mark_frame" style_key="e_check_mark_frame" anchor="left top bottom" anchor_measures="0 0 24 0"/>
		<entry element_name="text" style_key="e_field_text_p" anchor="top right bottom" anchor_measures="28 0 0 0"/>
	</map>

	<map key="e_list">
		<entry element_name="frame" style_key="e_field_frame"/>
	</map>

	<map key="e_list_item">
		<entry element_name="frame" style_key="e_list_item_frame"/>
		<entry element_name="selected_frame" style_key="e_list_item_selected_frame"/>
		<entry element_name="mouse_selected_frame" style_key="e_list_item_mouse_selected_frame"/>
		<entry element_name="text" style_key="e_list_item_text"/>
	</map>

	<map key="e_menu_list">
		<entry element_name="frame" style_key="e_field_frame"/>
	</map>

	<map key="e_menu_list_item">
		<entry element_name="frame" style_key="e_list_item_frame"/>
		<entry element_name="selected_frame" style_key="e_list_item_selected_frame"/>
		<entry element_name="mouse_selected_frame" style_key="e_list_item_mouse_selected_frame"/>
		<entry element_name="text" style_key="e_list_item_text"/>
	</map>

	<map key="e_combo">
		<entry element_name="frame" style_key="e_combo_frame"/>
		<entry element_name="text" style_key="e_field_text"/>
	</map>

	<map key="e_combo_list">
		<entry element_name="frame" style_key="e_field_frame"/>
	</map>

	<map key="e_combo_list_item">
		<entry element_name="frame" style_key="e_list_item_frame"/>
		<entry element_name="selected_frame" style_key="e_list_item_selected_frame"/>
		<entry element_name="mouse_selected_frame" style_key="e_list_item_mouse_selected_frame"/>
		<entry element_name="text" style_key="e_list_item_text"/>
	</map>

	<map key="e_scroll">
		<entry element_name="rail_frame" style_key="e_scroll_rail_frame"/>
		<entry element_name="grip_frame" style_key="e_scroll_grip_frame"/>
	</map>

	<map key="e_file_picker">
		<entry element_name="frame" style_key="e_panel_frame"/>
	</map>

	<map key="e_collection">
		<entry element_name="frame" style_key="e_field_frame"/>
		<entry element_name="mouse_selected_frame" style_key="e_list_item_outlined_frame"/>
		<entry element_name="last_selected_frame" style_key="e_list_item_outlined_frame"/>
		<entry element_name="column_frame" style_key="e_collection_column_frame"/>
		<entry element_name="column_text" style_key="e_collection_column_text"/>
		<entry element_name="item_selected_frame" style_key="e_collection_item_selected_frame"/>
		<entry element_name="item_icon_frame" style_key="e_collection_item_icon_frame"/>
		<entry element_name="item_text" style_key="e_collection_item_text"/>
	</map>

	<map key="e_property_inspector">
		<entry element_name="frame" style_key="e_field_frame"/>
	</map>

</styles>
